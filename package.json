{
    "name": "@sinonjs/samsam",
    "version": "3.3.3",
    "description": "Value identification and comparison functions",
    "homepage": "http://sinonjs.github.io/samsam/",
    "author": "Christian Johansen",
    "license": "BSD-3-Clause",
    "main": "./lib/samsam",
    "repository": {
        "type": "git",
        "url": "https://github.com/sinonjs/samsam.git"
    },
    "lint-staged": {
        "*.js": "eslint"
    },
    "scripts": {
        "benchmark": "node lib/deep-equal-benchmark.js",
        "build": "run-s build:dist-folder build:bundle",
        "build:bundle": "rollup -c > dist/samsam.js",
        "build:dist-folder": "mkdirp dist",
        "jsdoc": "jsdoc -c jsdoc.conf.json",
        "lint": "eslint .",
        "prepublishOnly": "npm run build && mkdocs gh-deploy -r upstream || mkdocs gh-deploy -r origin",
        "test": "mocha ./lib/*.test.js",
        "test-cloud": "npm run test-headless -- --wd",
        "test-coverage": "nyc --reporter text --reporter html --reporter lcovonly npm run test",
        "test-headless": "mochify lib/*.test.js"
    },
    "files": [
        "dist/",
        "docs/",
        "lib/",
        "!lib/**/*.test.js"
    ],
    "dependencies": {
        "@sinonjs/commons": "^1.6.0",
        "array-from": "^2.1.1",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@sinonjs/referee": "^3.2.0",
        "benchmark": "2.1.4",
        "eslint": "^6.2.0",
        "eslint-config-prettier": "^6.1.0",
        "eslint-config-sinon": "^3.0.1",
        "eslint-plugin-ie11": "^1.0.0",
        "eslint-plugin-jsdoc": "^15.8.3",
        "eslint-plugin-local-rules": "^0.1.1",
        "eslint-plugin-mocha": "^6.0.0",
        "eslint-plugin-prettier": "^3.1.0",
        "husky": "^0.14.3",
        "jsdoc": "^3.6.3",
        "jsdom": "^13.0.0",
        "jsdom-global": "^3.0.2",
        "lint-staged": "^6.1.0",
        "microtime": "2.1.8",
        "mkdirp": "^0.5.1",
        "mocha": "^6.2.0",
        "mochify": "^6.4.1",
        "npm-run-all": "^4.1.2",
        "nyc": "^14.1.1",
        "prettier": "^1.18.2",
        "proxyquire": "^2.1.3",
        "rollup": "^0.57.1",
        "rollup-plugin-commonjs": "^9.1.0",
        "sinon": "^7.4.2"
    }
}
